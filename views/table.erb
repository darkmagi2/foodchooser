<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item["id"] %></td>
        <td><%= item["name"] %></td>
        <td><%= item["description"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
